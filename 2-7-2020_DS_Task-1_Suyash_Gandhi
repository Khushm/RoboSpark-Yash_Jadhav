#include<stdio.h>
#include<string.h>
struct Students
{
	int stud_id;
	char stud_name[25];
	int stud_year;
	float stud_cgpa;
};
 
int main()
{
struct Students s[4];

int i;
for(i=0;i<4;i++)
	{
		printf("Enter the details of %dth student\n",i+1);
		printf("Enter Id of %dth student\n ",i+1);
		scanf("%d",&s[i].stud_id);
		fflush(stdin);
		printf("Enter name of student %dth\n",i+1);
		gets(s[i].stud_name);
		printf("Enter year of %dth student \n",i+1);
		scanf("%d",&s[i].stud_year);
		printf("Enter cgpa of %dth student \n",i+1);
		scanf("%f",&s[i].stud_cgpa);
		printf("\nEntered Details of %dth student\n",i+1);
		printf("\nID=%d\nNAME=%s\nYEAR=%d\nCGPA=%f\n\n",s[i].stud_id,s[i].stud_name,s[i].stud_year,s[i].stud_cgpa);
		
	}
}
